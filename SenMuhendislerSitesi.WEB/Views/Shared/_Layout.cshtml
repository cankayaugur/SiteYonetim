

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Şen Mühendisler Sitesi Başlığı</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato:300,300i,400,400i,700,700i,900|Playfair+Display|Roboto" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_libs/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_libs/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_libs/custom-icons/styles.css" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_libs/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_libs/animate.css" />
    <link rel="stylesheet" type="text/css" href="/assets/_assets/_css/style.css" />
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="loader">
        <div class="loader-spinner">
        </div>
    </div>
    <div id="form-popup">
        <div class="form-popup-background">
        </div>
        <div class="form-popup-content">
            <i class="fa fa-times close-popup-icon"></i>
            <h2>İletişim Kur</h2>
            <p class="centered">Soru Sormak İçin Bize Mesaj Gönderin.</p>
            <form method="post" class="form-active" data-title="New email from your website" data-client-name-field="Name" data-client-email-field="E-mail">
                <p class="left">
                    <input type="text" name="Name" placeholder="İsim..." class="form-field" />
                    <input type="text" name="Phone" placeholder="Telefon..." class="form-field" data-required="no" />
                    <input type="email" name="Email" placeholder="E-posta..." class="form-field" />
                </p>
                <p class="right">
                    <textarea name="message" rows="10" cols="5" placeholder="Mesaj..." class="form-field"></textarea>
                    <button type="button" class="button button-open-on-white form-send"><span>Gönder</span></button>
                </p>
                <div class="thanks-lay">
                    <div class="thanks-lay-helper">
                    </div>
                    <div class="thanks-lay-content">
                        <div class="thanks-lay-content-sending">
                            <div class="thanks-lay-icon"><i class="fa fa-cog fa-spin"></i></div>
                            Sending...

                        </div>
                        <div class="thanks-lay-content-sent">
                            <div class="thanks-lay-icon"><i class="fa fa-check"></i></div>
                            Your message has been sent, thank you!

                            <span class="thanks-lay-close">Close this notice</span>
                        </div>
                        <div class="thanks-lay-content-not-sent">
                            <div class="thanks-lay-icon"><i class="fa fa-times"></i></div>
                            Please try again.

                            <span class="thanks-lay-close">Close this notice</span>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <header class="header-bar">
        <div class="header-bar-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="header-bar-top-element"><i class="fa fa-phone"></i> <a href="tel:18001234567">0312 256 00 00</a></div>
                        <div class="header-bar-top-element"><i class="fa fa-envelope-o"></i> <a href="mailto:email@example.com">onemli.ugurcan@gmail.com</a></div>
                        <div class="header-bar-top-element"><i class="fa fa-map-marker"></i>Ergazi Mahallesi 1820.Cadde Batıkent/ANKARA</div>

                    </div>
                </div>
            </div>
        </div>
        <div class="header-bar-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="logo">
                            <a href="@Url.Action("Index","Anasayfa")">
                                <div class="logo-middle-helper">
                                </div>
                                <img src="~/assets/_assets/_img/senmuh.png" alt="Sen Muhendisler Sitesi" />
                            </a>
                        </div>
                        <nav>
                            <ul class="menu">
                                <li class="current-menu-item"><a href="/anasayfa">Anasayfa</a></li>
                                <li><a href="@Url.Action("Index","Duyuru")">Duyurular</a></li>
                                <li>
                                    <a href="@Url.Action("Index","YonetimKurulu")">Yönetim Kurulu <i class="fa fa-caret-down"></i></a>
                                    <ul class="sub-menu">
                                        <li><a href="@Url.Action("Index","GuncelYonetimKurulu")">Güncel Yönetim</a></li>
                                        <li><a href="@Url.Action("Index","GecmisYonetimKurulu")">Geçmiş Yönetimler</a></li>

                                    </ul>
                                </li>
                                <li><a href="@Url.Action("Index","Galeri")">Galeri</a></li>
                                <li><a href="@Url.Action("Index","Hakkimizda")">Site Hakkında</a></li>
                                <li><a href="/iletisim">İLETİŞİM</a></li>
                            </ul>
                        </nav>
                        <div class="responsive-menu-button">
                            <i class="fa fa-bars"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="big-wrapper">
        <div class="wrapper">
            @RenderBody()
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <footer>
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-9 vertical-align-middle">
                                        <nav>
                                            <ul class="menu">
                                                <li>
                                                    <a>Sayfalar</a>
                                                    <ul>
                                                        <li><a href="@Url.Action("Index","Anasayfa")">Anasayfa</a></li>
                                                        <li><a href="@Url.Action("Index","Duyuru")">Duyurular</a></li>
                                                        <li><a href="@Url.Action("Index","YonetimKurulu")">Yonetim Kurulu</a></li>
                                                        <li><a href="@Url.Action("Index","Galeri")">Galeri</a></li>
                                                        <li><a href="@Url.Action("Index","Hakkimizda")">Site Hakkında</a></li>
                                                        <li><a href="@Url.Action("Index","Iletisim")">İletişim</a></li>

                                                    </ul>
                                                </li>

                                            </ul>
                                        </nav>
                                    </div>
                                    <div class="col-md-3 vertical-align-middle">
                                        <p>2018 &copy; <a href="/Anasayfa/index/">Şen Mühendisler Sitesi</a></p>
                                    </div>
                                </div>
                            </div>
                        </footer>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/assets/_assets/_libs/jquery.js"></script>
    <script src="http://maps.google.com/maps/api/js" type="text/javascript"></script>
    <script src="/assets/_assets/_libs/bootstrap/js/bootstrap.min.js"></script>
    <script src="/assets/_assets/_libs/isotope.pkgd.min.js"></script>
    <script src="/assets/_assets/_libs/magnific-popup/magnific-popup.min.js"></script>
    <script src="/assets/_assets/_js/functions.js"></script>


    @RenderSection("script", false)
</body>
</html>